{
    "tags": {
        "allowUnknownTags": false
    },
    "source": {
        "include": ["data_structure"],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": ".+\\.test.js$"
    },
    "plugins": ["plugins/markdown"],
    "opts": {
        "template": "node_modules/docdash",
        "encoding": "utf8",
        "destination": "docs/",
        "recurse": true,
        "verbose": true
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    }
}
